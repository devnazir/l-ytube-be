{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "import/no-unresolved": "off",
    "func-names": "off",
    "no-unneeded-ternary": "off",
    "consistent-return": "off"
  }
}
